<!DOCTYPE html>
<html lang="en">
<%- include('./partials/head.ejs') %>

  <body class="contacts">
    <%- include('./partials/nav.ejs') %>
      <h1>Contacts:</h1>
      <% if (contacts.length) { %>
        <% contacts.forEach(({ link, name, image })=> { %>

          <i class="<%= image %>"></i>
          <a href="<%= link %>">
            <%= name %>
          </a>
          <br>
          <% }) %>
            <% } %>
  </body>

</html>